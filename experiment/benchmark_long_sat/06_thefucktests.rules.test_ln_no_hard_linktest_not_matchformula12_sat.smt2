(set-logic ALL)
(declare-const script String)
(declare-const output String)
(assert (not (< (str.len script) 0)))
(assert (not (str.prefixof "sudo " script)))
(assert (not (< (str.len output) 0)))
(assert (str.suffixof "hard link not allowed for directory" output))
(assert (not (< (str.len script) 0)))
(assert (not (= (+ 0 (str.indexof script "\u{5c} " 0)) (- 1))))
(check-sat)
(get-value (script))
(get-value (output))
